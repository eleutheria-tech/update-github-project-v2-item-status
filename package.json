{
  "name": "@eleutheria-tech/update-github-project-v2-item-status",
  "version": "0.0.5",
  "private": false,
  "description": "This action updates the single select field value of the items in your organization's GitHub Project.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eleutheria-tech/update-github-project-v2-item-status.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "June Eleutheria (https://github.com/june21x)",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^5.0.3"
  },
  "devDependencies": {
    "@octokit/graphql-schema": "^12.6.0",
    "@types/node": "^18.7.18",
    "@typescript-eslint/parser": "^5.45.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.23.1",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.0.4",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.8.3"
  }
}
